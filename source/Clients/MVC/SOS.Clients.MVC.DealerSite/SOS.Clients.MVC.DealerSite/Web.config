<?xml version="1.0"?>
<configuration>
	<appSettings>
		<add key="TEMPLATE_DIRECTORIES" value="~/js/views"/>
		<add key="RenderMode" value="Debug"/>
		<add key="Iis6StaticContentHack" value="true" />
	</appSettings>

	<system.web>
		<compilation debug="true" targetFramework="4.0" />
		<httpModules>
			<add name="TemplateModule" type="SOS.Clients.MVC.DealerSite.Support.TemplateModule, SOS.Clients.MVC.DealerSite"/>
		</httpModules>

		<identity impersonate="false" />
	</system.web>

	<system.webServer>
		<validation validateIntegratedModeConfiguration="false" />
		<modules runAllManagedModulesForAllRequests="true">
			<add name="TemplateModel" type="SOS.Clients.MVC.DealerSite.Support.TemplateModule" />
		</modules>
		<caching enabled="true" enableKernelCache="true">
			<profiles>
				<add extension=".html" policy="DisableCache" kernelCachePolicy="DisableCache" />
				<add extension=".js" policy="DisableCache" kernelCachePolicy="DisableCache" />
				<add extension=".css" policy="DisableCache" kernelCachePolicy="DisableCache" />
			</profiles>
		</caching>

	</system.webServer>
</configuration>
